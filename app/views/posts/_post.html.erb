<div id="<%= dom_id post %>">
  <p>
    <strong>Title:</strong>
    <%= post.title %>
  </p>

  <p>
    <strong>Content:</strong>
    <%= post.content %>
  </p>

  <p>
    <strong>Posted at:</strong>
    <%= post.posted_at %>
  </p>

  <p>
    <strong>Changed at:</strong>
    <%= post.changed_at %>
  </p>

</div>
